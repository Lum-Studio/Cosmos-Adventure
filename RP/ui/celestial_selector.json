  "green_launch_button": {
    "type": "collection_panel",
    "collection_name": "form_buttons",
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$toggle",
        "source_property_name": "#toggle_state",
        "target_property_name": "#selected"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#form_text - 'Tier ')",
        "target_property_name": "#tier"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#selected and (#tier > $tier or #tier = $tier))",
        "target_property_name": "#visible"
      }
    ],
    "controls": [
      {
        "launch_button@cosmos.button": {
          "size": [74, 11],
          "collection_index": "$index",
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "$action": "button.form_button_click",
          "bindings": "$form_buttons",
          "controls": [
            { 
              "default@selector.red_launch_button": {
                "color": [0, 1, 0]
              }
            },
            {
              "hover@selector.red_launch_button": {
                "color": [0.7, 1, 0.3]
              }
            }
          ]
        }
      }
    ]
  },

  "red_launch_button": {
    "type": "image",
    "texture": "textures/ui/cosmos/launch",
    "color": [1, 0, 0],
    "size": [74, 11],
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "controls": [
      {
        "launch_label": {
          "type": "label",
          "text": "LAUNCH",
          "layer": 3
        }
      }
    ]
  },

