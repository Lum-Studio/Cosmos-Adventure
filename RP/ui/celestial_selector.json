    ],
    "controls": [
      {
        "input_panel": {
          "type": "input_panel",
          "hover_enabled": false,
          "prevent_touch_input": false,
          "controls": [
            {
              "screen_background": {
                "type": "image",
				        "texture": "textures/ui/cosmos/space",
                "layer": -1
              }
            },
            {"catalog@selector.catalog": {}},
            {"launch_panel@selector.launch_panel": {}},
			      {"solar_system@selector.solar_system": {}}
          ]
        }
      }
    ]
  },

