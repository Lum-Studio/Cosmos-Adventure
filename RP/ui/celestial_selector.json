            }},
            {"venus@selector.catalog_select_and_zoom_toggle": {
              "$name": "Venus",
              "$control_name": "venus",
              "$tier": 3,
              "$time": 0.5,
              "$index": 2
            }},
            {"overworld@selector.catalog_select_and_zoom_toggle": {
              "$name": "Overworld",
